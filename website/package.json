{
  "name": "website",
  "version": "1.0.3",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "prettier --write '{docs,src}/**/*.{md,ts,tsx}' && docusaurus build --out-dir dist",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy --out-dir dist"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.61",
    "@docusaurus/plugin-google-analytics": "^2.0.0-alpha.61",
    "@docusaurus/preset-classic": "^2.0.0-alpha.61",
    "@docusaurus/theme-live-codeblock": "^2.0.0-alpha.61",
    "@formatjs/intl-displaynames": "^3.3.6",
    "classnames": "^2.2.6",
    "intl-messageformat": "^9.2.3",
    "intl-messageformat-parser": "^5.4.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-intl": "^5.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "arrowParens": "avoid",
    "semi": false
  },
  "devDependencies": {
    "prettier": "^2.0.5"
  }
}
